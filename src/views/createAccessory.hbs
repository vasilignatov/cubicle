<main>
    <h1>Create Accessory</h1>
    <div class="form">
        <form method="POST" action="/accessory/create">
            <label for="name">Name</label>
            <input type="text" id="name" name="name" value="{{oldData.name}}">
            <label for="name">Description</label>
            <textarea id="name" name="description">{{oldData.description}}</textarea>
            <label for="image">ImageUrl</label>
            <input type="text" id="image" name="imageUrl" value="{{oldData.imageURL}}">
            <input type="submit" value="Create">
        </form>
    </div>
</main>